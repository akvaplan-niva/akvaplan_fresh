{
  "tasks": {
    "dev": "deno run --unstable --allow-env --allow-read --allow-write=fresh.gen.ts,$HOME/.cache/fresh --allow-run --allow-net=:7777,:8000,deno.land,esm.sh,unpkg.com,www.mynewsdesk.com,svc.deno.dev,dois.deno.dev,mediebank.deno.dev,akvaplanists.deno.dev,datacite.deno.dev,research.deno.dev,api.datacite.org,api.openalex.org,api.cristin.no,fonts.gstatic.com --watch=static/,routes/ dev.ts",
    "prod": "DENO_DEPLOYMENT_ID=\"$(git rev-parse HEAD)\" deno run -A main.ts",
    "icons": "deno run --allow-net=fonts.gstatic.com --allow-read=static --allow-write=. tasks.ts icons",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "akvaplan_fresh/": "./",
    "std/": "https://deno.land/std@0.199.0/",
    "$fresh/": "https://deno.land/x/fresh@1.4.2/",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "accept-language-parser": "https://esm.sh/v115/accept-language-parser@1.5.0",
    "marked": "https://esm.sh/v115/marked@5.0.5",
    "marked/": "https://esm.sh/v115/marked@5.0.5/",
    "open-props": "https://esm.sh/v115/open-props@1.5.9",
    "open-props/": "https://esm.sh/v115/open-props@1.5.9/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.1",
    "preact-render-to-string/": "https://esm.sh/v115/*preact-render-to-string@6.2.0/",
    "preact": "https://esm.sh/preact@10.15.1",
    "preact/": "https://esm.sh/preact@10.15.1/"
  },
  "scopes": {},
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    },
    "exclude": [
      "_fresh"
    ]
  },
  "fmt": {
    "exclude": [
      "_fresh"
    ]
  }
}
